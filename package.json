{
  "name": "guidemaker-ember-template",
  "version": "3.5.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-learn/guidemaker-ember-template",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:one"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "algoliasearch": "^4.14.1",
    "broccoli-funnel": "^3.0.3",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-showdown": "^7.0.0",
    "ember-collapsible-panel": "^6.1.0",
    "ember-composable-helpers": "^4.4.1",
    "ember-concurrency": "^2.0.1",
    "ember-get-config": "^2.1.1",
    "ember-href-to": "^5.0.0",
    "ember-power-select": "^7.2.0",
    "ember-showdown-shiki": "^1.0.3",
    "ember-styleguide": "^10.0.0",
    "ember-tether": "^2.0.1",
    "ember-truth-helpers": "^3.0.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@percy/cli": "^1.0.0-beta.60",
    "@percy/ember": "^3.0.0-beta.1",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.4.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.0",
    "ember-fetch": "^8.1.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "guidemaker": "^3.1.0",
    "lint-to-the-future": "^2.0.0",
    "lint-to-the-future-ember-template": "^1.2.0",
    "lint-to-the-future-eslint": "^2.0.1",
    "lint-to-the-future-stylelint": "^1.1.0",
    "loader.js": "^4.7.0",
    "miragejs": "^0.1.48",
    "prember": "^2.0.0",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "release-plan": "^0.7.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": "~3.28 || ^4.0.0 || >=5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "fastbootDependencies": [
    "crypto"
  ]
}
