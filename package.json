{
  "name": "guidemaker-ember-template",
  "version": "2.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-learn/guidemaker-ember-template",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all test:*",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:one"
  },
  "dependencies": {
    "algoliasearch": "^3.30.0",
    "broccoli-funnel": "^2.0.1",
    "ember-auto-import": "^1.5.2",
    "ember-cli-babel": "^7.11.1",
    "ember-cli-htmlbars": "^4.0.5",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-showdown": "^4.4.4",
    "ember-collapsible-panel": "^3.1.1",
    "ember-composable-helpers": "^3.1.0",
    "ember-href-to": "^3.1.0",
    "ember-power-select": "^4.1.4",
    "ember-prism": "^0.4.0",
    "ember-styleguide": "^2.6.0-0",
    "ember-tether": "^1.0.0",
    "ember-truth-helpers": "^2.1.0",
    "sass": "^1.14.3"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.0.0",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.6",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "^2.0.4",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-concurrency": "^2.0.3",
    "ember-data": "~3.13.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^8.0.4",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-percy": "^1.5.0",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.3.0",
    "ember-source": "~3.13.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^10.0.0",
    "guidemaker": "^2.5.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.0.2",
    "qunit-dom": "^0.9.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
